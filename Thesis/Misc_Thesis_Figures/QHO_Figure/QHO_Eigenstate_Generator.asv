n = 0
x = linspace(-5, 5, 1000);

y = sqrt(1/((2^n) * factorial(n))) * ((m * omega)/(pi * hbar))^0.25 * exp(-(m * omega * x.^2)/(2 * hbar)) * hermiteH(n, (sqrt((m * omega)/hbar)));

